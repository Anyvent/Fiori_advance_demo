{
    "name": "@local/EEquipments",
    "private": true,
    "version": "1.0.0",
    "description": "UI5 Application: EEquipments",
    "author": "Stijn Puttemans",
    "license": "Apache-2.0",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "scripts": {
        "build": "ui5 build --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "build:opt": "npm-run-all build:ts build:ui5:opt",
        "build:ts": "babel src --out-dir webapp --source-maps inline --extensions \".ts,.js\" --copy-files",
        "build:ui5": "ui5 build --clean-dest",
        "build:ui5:opt": "ui5 build self-contained --clean-dest --all",
        "start": "npm-run-all --parallel watch:ts start:ui5",
        "start:launchpad": "npm-run-all --parallel watch:ts start:launch",
        "watch:ts": "babel src --out-dir webapp --source-maps true --extensions \".ts,.js\" --copy-files --watch",
        "start:ui5": "ui5 serve --port 8081 -o index.html",
        "start:launch": "ui5 serve --port 8080 -o launchpad.html",
        "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
        "ts-typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "deploy": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo",
        "build:mta": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy:abap": "npm run build:abap && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "build:ui5-cf": "ui5 build preload --clean-dest --config ui5-deploy-cf.yaml --include-task=generateManifestBundle generateCachebusterInfo",
        "build:ui5-abap": "ui5 build preload --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "build:abap":"npm-run-all build:ts build:ui5-abap",
        "build:cf": "npm-run-all build:ts build:ui5-cf",
        "undeploy": "cf undeploy pm-notif-man --delete-services --delete-service-keys"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-typescript": "^7.13.0",
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "@sap/ux-ui5-tooling": "1.6.7",
        "@sapui5/ts-types-esm": "1.90.0",
        "@types/dompurify": "^2.2.1",
        "@types/jquery": "3.5.1",
        "@types/qunit": "2.5.4",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "@ui5/cli": "^2.11.0",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "babel-preset-transform-ui5": "^7.0.3",
        "eslint": "^7.27.0",
        "mbt": "^1.0.15",
        "npm-run-all": "^4.1.5",
        "rimraf": "3.0.2",
        "typescript": "^4.2.3",
        "ui5-middleware-simpleproxy": "^0.9.1",
        "ui5-task-transpile": "^0.3.1",
        "ui5-task-zipper": "^0.3.1",
        "ui5-tooling-modules": "^0.7.0"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper",
            "ui5-task-transpile",
            "ui5-middleware-simpleproxy",
            "mbt",
            "ui5-tooling-modules"
        ]
    }
}
