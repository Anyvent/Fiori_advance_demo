<mvc:View
	controllerName="com.eliagroup.pm.pmeequipments.controller.Master"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<f:DynamicPage id="masterPage" 
			stickySubheaderProvider="iconTabBar"
			class="sapUiNoContentPadding">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="{i18n>appTitle}" level="H2"/>
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>
		<f:content>

		<Text text="{eEquipMan>/equipment/Equipment}"/>

		</f:content>
	</f:DynamicPage>
</mvc:View>